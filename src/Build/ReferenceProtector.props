<?xml version="1.0" encoding="utf-8"?>

<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">  
  <PropertyGroup>
    <EnableReferenceProtector Condition="'$(EnableReferenceProtector)' == ''">true</EnableReferenceProtector>
    <ReferenceProtectorTaskAssembly Condition="$(EnableReferenceProtector) != 'false'">$(MSBuildThisFileDirectory)ReferenceProtector.Tasks.dll</ReferenceProtectorTaskAssembly>
  </PropertyGroup>
  <ItemGroup>
    <CompilerVisibleProperty Include="MSBuildProjectFullPath" />
    <CompilerVisibleProperty Include="DependencyRulesFile" />
    <CompilerVisibleProperty Include="EnableReferenceProtector" />
  </ItemGroup>
</Project>